{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": ["s3:PutObject", "s3:GetObject", "s3:ListBucket"],
        "Resource": ["arn:aws:s3:::arxiv-raw-data-bucket", "arn:aws:s3:::arxiv-raw-data-bucket/*"]
      },
      {
        "Effect": "Allow",
        "Action": "logs:*",
        "Resource": "*"
      }
    ]
  }
  